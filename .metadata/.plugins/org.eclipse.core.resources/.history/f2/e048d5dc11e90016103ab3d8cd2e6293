
public class Literals {

	public static void main(String[] args) {
		String decimal="10";
		int ocatal=010;
		int hexdecimal=0x10;
		System.out.println("Decimal="+decimal);
		System.out.println("Ocatial="+ocatal);
		System.out.println("Hexdeciamal="+hexdecimal);
		
		int dec=Integer.parseInt(decimal,10);
//		int oct=Integer.parseInt("octal", ocatal);
//		int hex=Integer.parseInt("hexdecimal",hexdecimal);
//		
		System.out.println("DEC="+dec);
//		System.out.println("OCT="+oct);
//		System.out.println("HEX="+hex);
//				

	}

}
